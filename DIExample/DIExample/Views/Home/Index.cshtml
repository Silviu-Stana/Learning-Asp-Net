@model IEnumerable<string>
@inject ICitiesService citiesServiceInView

@{
    ViewBag.Title = "Home";
}

<h1>Cities</h1>
@{
    List<string> cities = citiesServiceInView.GetCities();
}

<ul class="list">
    @foreach (string city in cities)
    {
        <li>@city</li>
    }
</ul>


<div>@ViewBag.InstanceId_CitiesService</div>
<div>@ViewBag.InstanceId_CitiesService2</div>
<div>@ViewBag.InstanceId_CitiesService3</div>
<div class="box">@ViewBag.InstanceId_CitiesService_InScope</div>